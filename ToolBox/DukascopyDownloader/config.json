{
    //-------------------------------------------
    // Configuration for the Dukascopy Downloader
    //-------------------------------------------

    // Output folder in which files will be saved (usually the Lean data folder)
    "output-folder": "C:\\Lean\\Data",

    // List of symbols to download (if empty, all symbols are downloaded)
    // The list of symbols can be found in the first column of the instruments.txt file
    "instrument-list": [ "USA500IDXUSD", "DEUIDXEUR", "LIGHTCMDUSD", "XAUUSD", "EURUSD", "USDJPY" ],

    // Starting date for download requests (in yyyy-mm-dd format)
    "start-date": "2015-06-11",

    // Ending date for download requests (in yyyy-mm-dd format)
    "end-date": "2015-06-12",

    // Supported formats: "lean"
    "output-format": "lean",

    // If set to true, enables trace information displayed in the console
    "enable-trace": true
}